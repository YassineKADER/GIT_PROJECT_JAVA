<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.Pane?>

<Pane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ygit.Logs" stylesheets="style.css">
    <children>
        <TableView fx:id="loglist" layoutX="12.0" layoutY="49.0" onMouseClicked="#getItem" prefHeight="300.0" prefWidth="573.0" styleClass="table">
            <columns>
                <TableColumn fx:id="date" prefWidth="128.0" text="Date" style="-fx-text-fill: white;"/>
                <TableColumn fx:id="author" prefWidth="78.0" text="Author" style="-fx-text-fill: white;"/>
                <TableColumn fx:id="description" prefWidth="366.0" text="Description" style="-fx-text-fill: white;"/>
            </columns>
        </TableView>
        <TextArea fx:id="infos" layoutX="15.0" layoutY="363.0" prefHeight="158.0" prefWidth="567.0" styleClass="txtArea"/>
    </children>

</Pane>
